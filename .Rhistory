mean1.Hotelling <- function(X){
n       = nrow(X)
p       = ncol(X)
meanvec = colMeans(X)
output  = array(0,c(p,p))
for (i in 1:n){
cvec = as.vector(X[i,])-meanvec
output = output + outer(cvec,cvec)
}
output = output/(n-1)
return(output)
}
