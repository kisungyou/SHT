check_nd <- function(x){
cond1 = ((is.array(x))||(is.matrix(x)))
cond2 = (all(!is.infinite(x)))
cond3 = (all(!is.na(x)))
cond4 = (all(!is.complex(x)))
cond5 = (length(dim(x))==2) # only 2-dimensional array is allowed
cond6 = ((dim(x)[1]!=1)&&(dim(x)[2]!=1))
if (cond1&&cond2&&cond3&&cond4&&cond5&&cond6){
return(TRUE)
} else {
stop()
}
}
check_nd(a)
b = array(0,c(3,4))
check_nd(b)
